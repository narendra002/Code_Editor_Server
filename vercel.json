{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/$1"  // Adjust this based on your frontend build directory
    }
  ]
}
